    "@markdoc/markdoc": "^0.2.1",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^1.9.2",
    "astro-markdoc-renderer": "0.0.1-alpha.1",
    "clsx": "2.0.0",
    "cross-env": "^7.0.3",
    "globby": "^13.1.3",
    "gray-matter": "^4.0.3",
    "slugify": "^1.6.5",
    "svg-sprite": "^2.0.2",
    "tailwindcss": "^3.2.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "prettier-plugin-astro": "^0.7.2",
    "typescript": "^4.9.4"
