{
  "name": "blogster-bubblegum",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env CONTENT_DIR=content astro dev --host",
    "dev:demo": "cross-env CONTENT_DIR=content.demo astro dev --host",
    "build": "cross-env CONTENT_DIR=content astro build",
    "build:demo": "cross-env CONTENT_DIR=content.demo astro build",
    "preview": "astro preview",
